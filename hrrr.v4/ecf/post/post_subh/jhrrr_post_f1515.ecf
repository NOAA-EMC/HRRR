%include <head.h>
%include <envir-p2.h>

# EXPORT list here

set -x

export cyc=%CYC%
export fhr=15
export fmin=15
export job=hrrr_post_f1515_%CYC%

# versions file for Hi-Res Rapid Refresh model $mod_ver
VERSION_FILE=$NWROOT/versions/hrrr.ver
if [ -f $VERSION_FILE ]; then
  . $VERSION_FILE
else
  ecflow_client --abort
  exit
fi

# CALL executable job script here
$NWROOT/hrrr.${hrrr_ver}/jobs/JHRRR_POST_SUBH

if [ $? -ne 0 ]; then
  ecflow_client --abort
  exit
fi

%include <tail.h>
echo "Complete hrrr post job in ecflow"
date
